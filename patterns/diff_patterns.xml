<patterns>
 <class pattern="/\w+\.diff$" icon="unknown.png" name="diff" >

<!--Options-->
  <Option>
   <Name>wrap</Name>
   <Value>0</Value>
   <flags>2</flags>
  </Option>
  <Option>
   <Name>emulate_tabs</Name>
   <Value>1</Value>
   <flags>2</flags>
  </Option>
  <Option>
   <Name>default</Name>
   <Value>0</Value>
   <flags>2</flags>
  </Option>
  <Option>
   <Name>base_indentation</Name>
   <Value>0</Value>
   <flags>2</flags>
  </Option>

<!--Highlight styles-->
  <style format="2" name="delimiter" color="#cd0000" />
  <style format="0" name="in" color="#0000cd" />
  <style format="0" name="out" color="#00cd00" />

<!--Highlight patterns-->
  <range_pattern style="in" parent="" name="in" >
   <comments></comments>
   <begin>&lt;</begin>
   <end>$</end>
  </range_pattern>
  <range_pattern style="out" parent="" name="out" >
   <comments></comments>
   <begin>&gt;</begin>
   <end>$</end>
  </range_pattern>
  <keyword_pattern style="delimiter" parent="" name="delimiter" >
   <comments></comments>
   <keyword>^([0-9]+(,[0-9]+)?(a|c|d)[0-9]+(,[0-9]+)?)|(-+)\s*$</keyword>
  </keyword_pattern>

<!--Indentation patterns-->

<!--Parenthesis-->

<!--Block delimiters-->

<!--Text macros-->
 </class>
</patterns>
